{
  "version": 2,
  "builds": [
    {
      "src": "resources/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "resources/*.png",
      "use": "@vercel/static"
    },
    {
      "src": "resources/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "resources/*.wasm",
      "use": "@vercel/static"
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "out"
      }
    }
  ],
  "routes": [
    {
      "src": "/add_score",
      "dest": "/server.go"
    },
    {
      "src": "/index.html",
      "dest": "/resources/index.html"
    },
    {
      "src": "/logo.png",
      "dest": "/resources/logo.png"
    },
    {
      "src": "/restart.png",
      "dest": "/resources/logo.png"
    },
    {
      "src": "/logo.png",
      "dest": "/resources/logo.png"
    },
    {
      "src": "/1stplace.png",
      "dest": "/resources/logo.png"
    },
    {
      "src": "/wasm_exec.js",
      "dest": "/resources/wasm_exec.js"
    },
    {
      "src": "/blocks.wasm",
      "dest": "/out/blocks.wasm"
    },
    {
      "src": "/(.*)",
      "dest": "/resources/index.html"
    }
  ]
}
